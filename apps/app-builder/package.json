{
  "name": "metatonic-app-builder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "camelcase": "^5.0.0",
    "foundation-sites": "^6.4.4-rc1",
    "json-beautify": "^1.0.1",
    "metatonic-core": "^0.0.8",
    "metatonic-react": "^0.0.8",
    "metatonic-react-redux": "^0.0.8",
    "metatonic-redux": "^0.0.8",
    "react": "^16.3.2",
    "react-ace": "^6.1.1",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.14.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass": "^1.3.2"
  },
  "homepage": "http://beattyml1.github.io/metatonic-app-builder-demo",
  "scripts": {
    "sass": "node-sass ./src -o ./src",
    "sass-watch": "node-sass -w ./src -o ./src",
    "start": "npm run sass && npm run sass-watch &  react-scripts-ts start",
    "build": "npm run sass && react-scripts-ts build",
    "test": "CI=${CI} npm run sass && react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.8",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "gh-pages": "^1.1.0",
    "node-sass": "^4.9.0",
    "typescript": "^2.8.3"
  }
}
